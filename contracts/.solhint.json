{
  "extends": "solhint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "compiler-version": ["warn","^0.8.4"],
    "prettier/prettier": "error",
    "avoid-throw": "off",
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "not-rely-on-time": "off",
    "func-name-mixedcase": "off",
    "avoid-low-level-calls": "off"
  }
}
